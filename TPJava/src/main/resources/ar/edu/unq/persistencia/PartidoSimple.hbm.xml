<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="unq.tpi.persistencia.PartidoSimple" table="partidos" >
        <id name="id" type="java.lang.Integer">
            <column name="J_ID" />
            <generator class="identity" />
        </id>
        <property name="golesEquipo1" type="Integer">
            <column name="J_GOLES_PARTIDO1" length="3" not-null="true" unique="false" />
        </property>
        <property name="golesEquipo2" type="Integer">
            <column name="J_GOLES_PARTIDO2" length="3" not-null="true" unique="false" />
        </property>
    </class>
</hibernate-mapping>